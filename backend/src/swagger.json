        },
        "userFirstName": {
          "type": "string",
          "example": "Jacinthe"
        },
        "userLastName": {
          "type": "string",
          "example": "Windler"
        },
        "userBirthday": {
          "type": "string",
          "example": "1974-12-02"
        },
        "userPhone": {

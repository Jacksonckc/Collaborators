          "type": "string",
          "example": "Retta"
        },
        "userLastName": {
          "type": "string",
          "example": "McLaughlin"
        },
        "userBirthday": {
          "type": "string",
          "example": "2007-12-09"
        },
        "userPhone": {
          "type": "string",
          "example": "446-510-2772"
        },
        "userStory": {
          "type": "string",
          "example": "Rerum eveniet recusandae dolorem aut ratione debitis ab quos in. Reprehenderit sapiente commodi qui aperiam nostrum id magni cumque. Illo id modi totam nobis omnis."
        },
        "userAvatar": {
          "type": "string",
          "example": "Voluptatibus eaque omnis aliquid."
        },
        "password": {
          "type": "string",
          "example": "2Hortense95"
        },
        "userProjectCount": {
          "type": "number",
          "example": 1
        },
        "acorns": {
          "type": "number",
          "example": 5
        },
        "userLevel": {
          "type": "number",
          "example": 1
        }
      },
      "required": [
        "userEmail",
        "userFirstName",
        "password",
        "userProjectCount",
        "acorns",
        "userLevel"
      ]
    },
    "Password": {
      "type": "object",
      "properties": {

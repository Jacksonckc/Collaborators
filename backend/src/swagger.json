  "definitions": {
    "Project": {
      "type": "object",
      "properties": {
        "projectName": {
          "type": "string",
          "example": "Earum perferendis sed tempore eaque eveniet autem quidem et."
        },
        "projectLink": {
          "type": "string",
          "example": "Kitty.Littel@hotmail.com"
        },
        "projectAutherId": {
          "type": "string",
          "example": "e85f42ac-3e6b-46f4-9242-0776ad3b7f85"
        },
        "projectLikesCount": {
          "type": "number",
          "example": 29
        },
        "projectStartDate": {
          "type": "string",
          "example": "1970-01-15"
        },
        "projectDescription": {
          "type": "string",
          "example": "Asperiores qui eius laboriosam. Consequatur qui non delectus consequatur expedita beatae. Facere suscipit dolore fugit sit ipsam. Molestiae numquam nihil et."
        },
        "isProjectFinished": {
          "type": "boolean",
          "example": false
        }
      },
      "required": [
        "projectName",
        "projectLink",
        "projectAutherId",
        "projectLikesCount",
        "projectStartDate",
        "projectDescription",
        "isProjectFinished"
      ]
    },
    "Skill": {

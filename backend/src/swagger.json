          "example": "Marianna"
        },
        "userLastName": {
          "type": "string",
          "example": "Eichmann"
        },
        "userBirthday": {
          "type": "string",
          "example": "1999-06-10"
        },
        "userPhone": {
          "type": "string",
          "example": "516-396-4205"
        },
        "userStory": {
          "type": "string",
          "example": "Atque et error omnis quaerat odit. Quisquam dolores repellendus expedita quos esse ut dolorem consequatur. In aut amet maiores sed molestiae dolores vel voluptas."
        },
        "userAvatar": {
          "type": "string",
          "example": "Velit omnis ducimus enim voluptatibus ad."
        },
        "password": {
          "type": "string",
          "example": "7Morris46"
        },
        "userProjectCount": {
          "type": "number",
          "example": 4
        },
        "acorns": {
          "type": "number",
          "example": 0
        },
        "userLevel": {
          "type": "number",
          "example": 1
        }
      },
      "required": [
        "userEmail",
        "userFirstName",
        "password",
        "userProjectCount",
        "acorns",
        "userLevel"
      ]
    },
    "Password": {
      "type": "object",
      "properties": {

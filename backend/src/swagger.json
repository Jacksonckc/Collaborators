  "definitions": {
    "Project": {
      "type": "object",
      "properties": {
        "projectName": {
          "type": "string",
          "example": "Odit doloribus aut nam dolores."
        },
        "projectLink": {
          "type": "string",
          "example": "Sean_Cole@Wuckert.io"
        },
        "projectAutherId": {
          "type": "string",
          "example": "692d2711-3aa4-4717-908c-e7a3e4f5451a"
        },
        "projectLikesCount": {
          "type": "number",
          "example": 98
        },
        "projectStartDate": {
          "type": "string",
          "example": "1994-04-03"
        },
        "projectDescription": {
          "type": "string",
          "example": "Nihil dolorem sed repudiandae voluptatem et quia quidem. Ut ipsa qui. Aperiam beatae iure quae. Porro velit voluptatem distinctio dolores maxime pariatur ab maiores odio. Eum eum repudiandae."
        },
        "isProjectFinished": {
          "type": "boolean",
          "example": true
        }
      },
      "required": [
        "projectName",
        "projectLink",
        "projectAutherId",
        "projectLikesCount",
        "projectStartDate",
        "projectDescription",
        "isProjectFinished"
      ]
    },
    "Skill": {
      "type": "object",
      "properties": {

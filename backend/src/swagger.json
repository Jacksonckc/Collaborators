        },
        "userAvatar": {
          "type": "string",
          "example": "Expedita ducimus debitis iste animi non laborum eum."
        },
        "password": {
          "type": "string",
          "example": "0Granville38"
        },
        "userProjectCount": {
          "type": "number",
          "example": 8
        },
        "acorns": {
          "type": "number",
          "example": 2
        },
        "userLevel": {
          "type": "number",
          "example": 1
        }
      },
      "required": [
        "userEmail",
        "userFirstName",
        "password",
        "userProjectCount",
        "acorns",
        "userLevel"
      ]
    },
    "Password": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string",
          "example": "b86361f7-d10f-4399-b361-ebd328e79f18"
        },
        "hash": {
          "type": "string",
          "example": "5Josh58"
        }
      },

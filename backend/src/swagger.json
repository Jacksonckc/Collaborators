  "definitions": {
    "Project": {
      "type": "object",
      "properties": {
        "projectName": {
          "type": "string",
          "example": "Placeat velit deleniti tempore molestias consequatur veritatis nostrum voluptatem rerum."
        },
        "projectLink": {
          "type": "string",
          "example": "Dewitt_Gutmann@Monte.ca"
        },
        "projectAutherId": {
          "type": "string",
          "example": "b29ce66d-4002-4623-91cf-c8348dc41c29"
        },
        "projectLikesCount": {
          "type": "number",
          "example": 99
        },
        "projectStartDate": {
          "type": "string",
          "example": "1978-04-03"
        },
        "projectDescription": {
          "type": "string",
          "example": "Sapiente aut soluta vel quis illo in. Molestiae vero quas sunt quasi. Occaecati tempora neque omnis enim esse labore nobis autem. Voluptatibus dolore dolores voluptatem vel ea."
        },
        "isProjectFinished": {
          "type": "boolean",
          "example": false
        }
      },
      "required": [
        "projectName",
        "projectLink",
        "projectAutherId",
        "projectLikesCount",
        "projectStartDate",
        "projectDescription",
        "isProjectFinished"
      ]
    },
    "Skill": {
